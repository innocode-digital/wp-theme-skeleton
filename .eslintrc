{
  "parser": "babel-eslint",
  "extends": ["airbnb-base",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jquery": true,
    "es6": true,
    "serviceworker": true,
    "worker": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.7.0"
    }
  },
  "rules": {
    "no-mixed-spaces-and-tabs": ["error", "smart-tabs"],
    "react/prop-types": 0
  }
}
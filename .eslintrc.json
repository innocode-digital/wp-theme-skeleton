{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es6": true,
        "worker": true,
        "amd": true,
        "jquery": true,
        "serviceworker": true
    },
    "settings": {
        "import/core-modules": ["jquery"]
    },
    "rules": {
        "react/prop-types": 0
    }
}
